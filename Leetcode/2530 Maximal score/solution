class Solution {
public:
    long long maxKelements(vector<int>& nums, int k) {
        priority_queue<int>q;
        for(int i=0;i<nums.size();i++)
        {
            q.push(nums[i]);
        }
        long long score=0;
        while(k--)
        {
            int f=q.top();
            q.pop();
            score+=f;
            // cout<<f<<endl;
            f=ceil(f/3.0);
            q.push(f);
        }

        return score;
    }
};
